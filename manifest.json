{
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "description": "Simple new tab",
   "manifest_version": 2,
   "minimum_chrome_version": "0",
   "name": "New Tab Pro",
   "offline_enabled": true,

   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

   "permissions": [
      "bookmarks", "http://bnm.md/*", "http://worldweatheronline.com/", "http://sypexgeo.net/", "http://ajax.googleapis.com/*", "https://google.com/", "http://127.0.0.1/", "http://www.calend.ru/"
   ],
   
   "version": "1.1.1",
   "web_accessible_resources": [ "index.html" ],
   
   "browser_action":{
    "default_icon":"icon128.png"
   }
}
